{
  "name": "apis",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:create": "mikro-orm migration:create",
    "db:init": "mikro-orm migration:fresh --seed",
    "db:test": "NODE_ENV=test mikro-orm migration:fresh --seed",
    "prepare": "ts-patch install"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@mikro-orm/cli": "5.6.0",
    "@mikro-orm/core": "^5.6.0",
    "@mikro-orm/migrations": "^5.6.0",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.6.0",
    "@mikro-orm/reflection": "5.6.0",
    "@mikro-orm/seeder": "5.6.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/throttler": "^4.0.0",
    "@types/ffmpeg": "^1.0.4",
    "@willsoto/nestjs-prometheus": "^5.1.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "fluent-ffmpeg": "^2.1.2",
    "joi": "^17.7.0",
    "minio": "^7.0.32",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "openai": "^3.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jest": "28.1.8",
    "@types/minio": "^7.0.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "eslint-config-custom": "workspace:^",
    "jest": "28.1.3",
    "nodemon": "^2.0.22",
    "supertest": "^6.1.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig": "workspace:^",
    "types": "workspace:^",
    "typescript": "4.9.5"
  }
}
